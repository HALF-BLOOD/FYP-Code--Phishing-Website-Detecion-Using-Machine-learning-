{
  "manifest_version": 2,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "Detects phishing websites and blocks access to them.",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://127.0.0.1:5000/"
  ],
  "browser_action": {
    "default_icon": "Images/logo.png",
    "default_popup": "popup.html"
  },


  "icons": {
    "48": "Images/logo.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
