{
  "manifest_version": 2,
  "name": "Phishing Detector",
  "version": "1.0",



  "permissions": [
    "activeTab",
    "http://127.0.0.1:5000/*",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],

  "browser_action": {
    "default_title": "Check for Phishing",
    "default_popup": "popup.html"
  }
}
